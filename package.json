{
  "name": "typescript-react-library-starter",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Scaffold for writing react-related library in typescript with coooool tools.",
  "repository": {
    "type": "git",
    "url": "https://github.com/menthays/typescript-react-library-starter.git"
  },
  "scripts": {
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "test": "jest",
    "build": "pack build",
    "publish": "pack publish"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.6.0",
    "@pika/pack": "0.3.7",
    "@pika/plugin-build-node": "0.6.1",
    "@pika/plugin-build-types": "0.6.1",
    "@pika/plugin-build-web": "0.6.1",
    "@pika/plugin-standard-pkg": "0.6.1",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.5",
    "docz": "1.3.2",
    "docz-theme-default": "1.2.0",
    "jest": "24.9.0",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-hooks-testing-library": "0.5.0",
    "react-testing-library": "7.0.0",
    "typescript": "3.6.4"
  }
}
